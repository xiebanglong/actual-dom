{
  "private": true,
  "version": "0.0.3",
  "packageManager": "pnpm@8.0.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "pnpm -r --parallel --filter='./packages/*' run dev",
    "playground": "pnpm -C playground dev",
    "build": "pnpm -r --filter='./packages/*' run build",
    "b": "node -r sucrase/register scripts/build.ts",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm -r publish"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "devDependencies": {
    "@actual-dom/babel-utils": "workspace:*",
    "@actual-dom/shared": "workspace:*",
    "@babel/core": "^7.22.11",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/preset-typescript": "^7.22.15",
    "@babel/traverse": "^7.22.17",
    "@babel/types": "^7.22.11",
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@release-it/conventional-changelog": "^7.0.2",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/babel__core": "^7.20.1",
    "@types/babel__helper-module-imports": "^7.18.0",
    "@types/babel__traverse": "^7.20.1",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "actual-dom": "workspace:*",
    "babel-plugin-actual-dom": "workspace:*",
    "babel-plugin-dom": "workspace:*",
    "build.config.ts": "workspace:*",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "unbuild": "^2.0.0",
    "vite-plugin-jsx": "workspace:*",
    "vitest": "^0.34.6"
  },
  "license": "MIT"
}
